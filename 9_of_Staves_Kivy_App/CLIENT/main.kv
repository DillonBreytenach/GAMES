<Page>:
    MDFloatLayout:
        orientation: 'vertical'

        Image:
            pos_hint: {'x': 0.0,'y': -0.2}
            size_hint: 1.5, 1.5
            source: "ASSETS/BACKGROUND.jpeg"
            allow_stretch: True

        Button:
            text:'9 of Staves'
            font_size: 29
            background_normal: 'ASSETS/wood1.jpeg'
            color: (0, 0, 0, 1)
            size_hint: .8, .1
            pos_hint: {'x': 0.105,'y': 0.85}


        TextInput:
            hint_text: "NAME"
            size_hint: .8, .15
            pos_hint: {'x': 0.105,'y': 0.65}# 0.2, .4

        Button:
            text: "START"
            background_normal: "ASSETS/wood2.jpeg"
            font_size: 25
            color: (210/255, 151/255, 32/255, 1)#USE COLOR PICKER
            size_hint: .8, .20
            pos_hint: {'x': 0.105,'y': 0.4}# 0.2, .4
            on_press: root.move()

        Button:
            id: spc
            background_normal: "ASSETS/wood2.jpeg"
            font_size: 25
            color: (0, 0, 0, 1)
            text: "SP_CARDS"
            size_hint: .8, .20
            pos_hint: {'x': 0.105,'y': 0.1}
            on_press: root.ads()

<AdsBank>:
    name: "AdsBank"
    id: AdsBank
    MDFloatLayout:
        Button: 
            text: "PLAYING_AD"
            id: back
            on_press: root.play()


<Score>:
    name: "Score"
    id: Score
    Button:
        id: back
        text:'GAME_OVER'
        on_press: root.home()

<MainWidget>:
    name: "MainWidget"
    id: MainWidget
    MDFloatLayout:

        orientation:'vertical'

    
        Image:
            pos_hint: {'x': 0.0,'y': -0.2}
            size_hint: 1.5, 1.5
            source: "ASSETS/BACKGROUND.jpeg"
            allow_stretch: True

        Button:
            id: OPP
            background_normal: "ASSETS/wood3.jpeg"
            text: 'LOBBY'
            size_hint: .4, .08
            pos_hint: {'x': 0, 'y': .92}

        Button:
            id: POINTS
            background_normal: "ASSETS/wood4.jpeg"
            text: 'POINTS'
            size_hint: .4, .08
            pos_hint: {'x': .4, 'y': .92}
        
        Button:
            id: QUIT
            text: "QUIT"
            size_hint: .15, .1
            pos: 240, 500
            on_press: root.home_it()







